{
  "name": "KiFi Beta",
  "version": "2.3.47",
  "manifest_version": 2,
  "description": "KiFi Private Beta",
  "icons": {
    "48": "icons/kifi.48.png",
    "128": "icons/kifi.128.png",
    "256": "icons/kifi.256.png"
  },
  "options_page": "options.html",
  "page_action": {
    "default_icon": "icons/keep.faint.png",
    "default_title": "KiFi"
  },
  "background" : {
    "page": "main.html"
  },
  "content_scripts" : [
    {
      "matches": ["*://*/*"],
      "js": ["scripts/ready.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "bookmarks",
    "http://*/",
    "https://*/"
  ],
  "web_accessible_resources": [
    "images/chatter.png",
    "images/globe.png",
    "images/kifi_logo.png",
    "images/shade_above.png",
    "images/snapshot.png",
    "images/social_icons.png",
    "images/metro/gear.png",
    "images/metro/slider.png",
    "images/metro/tile_kept.png",
    "images/metro/tile_logo.png",
    "images/results/dropbox.png",
    "images/results/facebook.png",
    "images/results/gdocs.gif",
    "images/results/gdrive.png",
    "images/results/gmail.png",
    "html/friend_card.html",
    "html/notify_box.html",
    "html/metro/comment.html",
    "html/metro/comments.html",
    "html/metro/notice_comment.html",
    "html/metro/notice_message.html",
    "html/metro/notices.html",
    "html/metro/compose.html",
    "html/metro/general.html",
    "html/metro/keepers.html",
    "html/metro/message.html",
    "html/metro/messages.html",
    "html/metro/pane.html",
    "html/metro/pane_comments.html",
    "html/metro/pane_general.html",
    "html/metro/pane_notices.html",
    "html/metro/pane_thread.html",
    "html/metro/pane_threads.html",
    "html/metro/slider2.html",
    "html/metro/thread.html",
    "html/metro/threads.html",
    "html/comments/snapshot_bar.html",
    "html/search/chatter.html",
    "html/search/friends.html",
    "html/search/google.html",
    "html/search/google_hits.html",
    "html/search/google_hit.html"
  ]
}
