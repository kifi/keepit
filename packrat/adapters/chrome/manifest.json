{
  "name": "KiFi Beta",
  "version": "see build.properties",
  "manifest_version": 2,
  "description": "KiFi Private Beta",
  "icons": {
    "48": "icons/kifi.48.png",
    "128": "icons/kifi.128.png",
    "256": "icons/kifi.256.png"
  },
  "options_page": "options.html",
  "page_action": {
    "default_icon": {
      "38": "icons/keep.faint.png"
    },
    "default_title": "KiFi"
  },
  "background": {
    "page": "main.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["scripts/ready.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "bookmarks",
    "http://*/",
    "https://*/"
  ],
  "web_accessible_resources": []
}
