{
  "name": "Kifi Beta",
  "version": "2.2.8",
  "manifest_version": 2,
  "description": "Keep it, Find it!",
  "icons": {
    "48": "icons/kifi.48.png",
    "128": "icons/kifi.128.png",
    "256": "icons/kifi.256.png"
  },
  "options_page": "options.html",
  "page_action": {
    "default_icon": "icons/keep.faint.png",
    "default_title": "KiFi"
  },
  "background" : {
    "page": "main.html"
  },
  "content_scripts" : [
    {
      "matches": ["*://*/*"],
      "js": ["scripts/ready.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "bookmarks",
    "http://*/",
    "https://*/"
  ],
  "web_accessible_resources": [
    "images/chatter.png",
    "images/convo.png",
    "images/globe.png",
    "images/kifilogo.png",
    "images/shade_above.png",
    "images/snapshot.png",
    "images/social_icons.png",
    "images/triangle_down.31x16.png",
    "images/results/dropbox.png",
    "images/results/facebook.png",
    "images/results/gdocs.gif",
    "images/results/gdrive.png",
    "images/results/gmail.png",
    "html/kept_hover.html",
    "html/main_hover.html",
    "html/social_hover.html",
    "html/footer.html",
    "html/comments/hearts.html",
    "html/comments/comments_list.html",
    "html/comments/comments_view.html",
    "html/comments/reply.html",
    "html/comments/comment.html",
    "html/comments/reply_list.html",
    "html/comments/thread_info.html",
    "html/comments/thread.html",
    "html/comments/message_list.html",
    "html/comments/comment_post.html",
    "html/comments/message_post.html",
    "html/comments/snapshot_bar.html",
    "html/search/chatter.html",
    "html/search/google.html",
    "html/search/google_hits.html",
    "html/search/google_hit.html"
  ]
}
