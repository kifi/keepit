{
  "name": "Kifi Beta",
  "version": "see build.properties",
  "manifest_version": 2,
  "description": "Kifi Private Beta",
  "icons": {
    "48": "icons/kifi.48.png",
    "128": "icons/kifi.128.png",
    "256": "icons/kifi.256.png"
  },
  "options_page": "options.html",
  "page_action": {
    "default_icon": {
      "38": "icons/keep.faint.png"
    },
    "default_title": "Kifi"
  },
  "background": {
    "page": "main.html"
  },
  "content_scripts": [],
  "permissions": [
    "tabs",
    "bookmarks",
    "webNavigation",
    "http://*/",
    "https://*/"
  ],
  "web_accessible_resources": []
}
