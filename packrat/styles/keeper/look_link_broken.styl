.kifi-llb-dialog
  position: fixed
  top: 0
  left: 0
  width: 100%
  height: 100%
  z-index: 99999999999998
  text-align: center

.kifi-llb-dialog-box
  position: relative
  margin: 0 auto
  top: 50%
  width: 460px
  box-shadow: rgba(52, 73, 94, .18) 5px 8px
  border: 1px solid #dbe1e7
  border-radius: 5px !important
  background: #fff
  color: #000
  padding: 78px 60px 88px 80px
  overflow: hidden
  text-align: left
  opacity: 0
  -webkit-transform: scale(.7,.7)
  transform: scale(.7,.7)
  transition: opacity .2s linear, -webkit-transform .2s ease-in
  transition: opacity .2s linear, transform .2s ease-in

.kifi-showing>.kifi-llb-dialog-box
  opacity: 1
  -webkit-transform: none
  transform: none
  transition-timing-function: linear, ease-out

.kifi-llb-dialog-box::before
  content: ' '
  position: absolute
  top: 0
  left: 0
  right: 0
  height: 42px
  background: #eff2f7 16px 12px/32px 18px no-repeat
  background-image: url("data:image/png;base64,")

@media (min-resolution: 1.3dppx)
  .kifi-llb-dialog-box::before
    background-image: url("data:image/png;base64,")

.kifi-llb-dialog-title
  font-size: 22px
  line-height: 31px
  color: #333332
  font-weight: 500
  margin-bottom: 18px
  margin-left: -48px
  padding-left: 48px
  background: left center/31px 31px no-repeat
  background-image: url("data:image/png;base64,")

@media (min-resolution: 1.3dppx)
  .kifi-llb-dialog-title
    background-image: url("data:image/png;base64,")

.kifi-llb-dialog-body
  font-size: 16px
  line-height: 28px
  color: #444

.kifi-llb-dialog-img-a
  display: block
  margin: 20px 0 0

.kifi-llb-dialog-img
  max-width: 400px
  max-height: 200px

.kifi-llb-dialog-quote
  margin-top: 20px
  max-height: 200px
  overflow: auto

.kifi-llb-dialog-p+.kifi-llb-dialog-pp
  padding-top: 6px

.kifi-llb-dialog-p
  border-left: 3px solid #CCC
  padding-left: 10px
  font-size: 14px
  line-height: 18px

.kifi-llb-dialog-btn
  position: absolute
  bottom: 21px
  right: 24px
  background-color: #6fa6ef
  border-radius: 2px !important
  line-height: 41px
  color: #fff !important
  padding: 0 24px

.kifi-llb-dialog-btn:hover
  background-color: #7faeef

.kifi-llb-dialog-btn:active
  background-color: #5f9cef

.kifi-llb-dialog-btn:focus
  outline: 1px dotted #ccc !important

.kifi-llb-dialog-x
  position: absolute
  top: 9px
  right: 10px
  height: 24px
  width: 24px
  border-radius: 50% !important

.kifi-llb-dialog-x:before,
.kifi-llb-dialog-x:after
  content: ' '
  position: absolute
  top: 4px
  bottom: 4px
  left: 50%
  width: 2px
  background: #b1b8c1
  font-size: 0

.kifi-llb-dialog-x:before
  -webkit-transform: translate(-1px) rotate(45deg)
  transform: translate(-1px) rotate(45deg)

.kifi-llb-dialog-x:after
  -webkit-transform: translate(-1px) rotate(-45deg)
  transform: translate(-1px) rotate(-45deg)

.kifi-llb-dialog-x:hover:before,
.kifi-llb-dialog-x:hover:after,
.kifi-llb-dialog-x:focus:before,
.kifi-llb-dialog-x:focus:after
  background: #8e939a

.kifi-llb-dialog-x:active:before,
.kifi-llb-dialog-x:active:after
  background: #fff

.kifi-llb-dialog-x:active
  background: #d0d4da

