.kifi-message-mute-option
  position: relative
  color: white

.kifi-message-mute-option-mute,
.kifi-message-mute-option-unmute
  padding: 12px 12px 12px 60px
  cursor: pointer

.kifi-message-mute-option-unmute
  display: none

.kifi-message-header.kifi-muted  .kifi-message-mute-option-mute
  display: none

.kifi-message-header.kifi-muted  .kifi-message-mute-option-unmute
  display: block

.kifi-message-mute-option-icon
  position: absolute
  top: 12px
  left: 16px

.kifi-message-mute-option-title
  font-size: 18px
  font-weight: 200
  letter-spacing: .5px

.kifi-message-mute-option-description
  font-size: 13px
  font-weight: 200
  -webkit-font-smoothing: antialiased
  margin-top: 4px

.kifi-message-header-muted
  display: none
  font-size: 12px
  font-weight: 300
  line-height: 23px
  padding: 8px
  padding-left: 22px
  background: #f6cd94
  color: white
  overflow: hidden

.kifi-muted .kifi-message-header-muted
  display: block

.kifi-message-header-muted-icon
  display: inline-block
  margin-right: 5px

.kifi-message-mute-option-icon,
.kifi-message-header-muted-icon
  width: 24px
  height: 23px
  background-repeat: no-repeat
  background-image: url("data:image/png;base64,")
  background-size: auto 23px
  background-position: 50%
  vertical-align: middle

.kifi-muted .kifi-message-mute-option-icon
  background-image: url("data:image/png;base64,")

@media (min-resolution: 1.3dppx)
  .kifi-message-mute-option-icon,
.kifi-message-header-muted-icon
    background-image: url("data:image/png;base64,")

  .kifi-muted .kifi-message-mute-option-icon
    background-image: url("data:image/png;base64,")

.kifi-message-header-unmute-button
  float: right
  padding: 4px 11px
  line-height: 15px
  color: white
  background: #f0ad4d
  text-decoration: none
  border-radius: 2px !important

.kifi-message-header-unmute-button:hover,
.kifi-message-header-unmute-button:focus
  color: white
  background: #d89c45

.kifi-message-header-unmute-button:active
  color: white
  background: #c08a3e

