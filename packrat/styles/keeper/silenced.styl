.kifi-silenced
  position: fixed
  z-index: 999999999991
  right: 12px
  top: 10px
  width: 450px
  border: 2px solid #b8c5d2
  border-radius: 5px !important
  box-shadow: rgba(0,40,90,.2) 5px 4px 0 2px
  padding: 20px 0 28px
  font-size: 14px
  line-height: 21px
  color: #444
  -webkit-font-smoothing: antialiased
  opacity: 0
  -webkit-transform-origin: center top
  transform-origin: center top
  -webkit-transform: scale(.3,.3)
  transform: scale(.3,.3)
  transition: opacity .2s linear, -webkit-transform .2s ease-in
  transition: opacity .2s linear, transform .2s ease-in
  background: #fff 12px 12px/24px 14px no-repeat
  background-image: url("data:image/png;base64,")

@media (min-resolution: 1.3dppx)
  .kifi-silenced
    background-image: url("data:image/png;base64,")

.kifi-silenced.kifi-showing
  opacity: 1
  -webkit-transform: none
  transform: none
  transition-timing-function: linear, ease-out

.kifi-silenced-body
  padding-left: 88px

.kifi-silenced-h
  line-height: 24px
  font-size: 18px
  color: #6fa6ef
  margin-bottom: 16px

.kifi-silenced-tip
  margin: 16px 0 0

.kifi-silenced-tip::before
  content: 'Tip: '
  font-size: 13px
  font-weight: bold
  margin-right: 4px

.kifi-silenced-k
  display: inline-block
  vertical-align: middle
  width: 19px
  height: 19px
  background-image: url("data:image/png;base64,")
  background-size: 19px 19px
  margin: 0 2px

.kifi-silenced-x
  position: absolute
  top: 7px
  right: 7px
  height: 22px
  width: 22px
  border-radius: 50% !important

.kifi-silenced-x:before,
.kifi-silenced-x:after
  content: ' '
  position: absolute
  top: 4px
  bottom: 4px
  left: 50%
  width: 1px
  background: #b1b8c1
  font-size: 0

.kifi-silenced-x:before
  -webkit-transform: translate(-.5px) rotate(45deg)
  transform: translate(-.5px) rotate(45deg)

.kifi-silenced-x:after
  -webkit-transform: translate(-.5px) rotate(-45deg)
  transform: translate(-.5px) rotate(-45deg)

.kifi-silenced-x:hover:before,
.kifi-silenced-x:hover:after,
.kifi-silenced-x:focus:before,
.kifi-silenced-x:focus:after
  background: #8e939a

.kifi-silenced-x:active:before,
.kifi-silenced-x:active:after
  background: #fff

.kifi-silenced-x:active
  background: #d0d4da

