.kf-org-profile-libraries
  margin: 15px

.kf-upl-move-card-text
.kf-upl-create-card-text
  font-size: 14px
  text-transform: uppercase

.kf-upl-card.kf-opl-move-libraries
  position: relative
  background-color: #e7e7e7
  border: 2px dashed #979797
  line-height: 23px

  .kf-opl-move-libraries-wrapper
    display: flex
    width: 100%
    height: 100%
    justify-content: center
    align-items: center

.kf-opl-move-libraries-box
  display: block
  width: 60px
  height: 60px
  margin: 0 auto
