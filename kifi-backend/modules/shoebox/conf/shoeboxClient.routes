#################
# CLIENT routes #
#################

POST    /client/1/keeps/send            @com.keepit.controllers.client.ClientKeepMutationController.createKeep()
POST    /client/1/keeps/:id/recipients  @com.keepit.controllers.client.ClientKeepMutationController.modifyKeepRecipients(id: PublicId[Keep])

GET    /client/1/keeps/:keepId          @com.keepit.controllers.client.ClientKeepInfoController.getNewKeepView(keepId: PublicId[Keep])
GET    /client/1/keeps/stream           @com.keepit.controllers.client.ClientKeepInfoController.getKeepStream(fromId: Option[String] ?= None)
