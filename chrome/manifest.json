{
  "name": "Kifi",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Keep it, Find it!",
  "icons" : {
    "256" : "kifi.png"
  },
  "options_page": "options.html",
  "page_action": {
    "default_icon": "keepit.png", // optional
    "default_title": "Kifi"//,    // optional; shown in tooltip
    //"default_popup": "keep.html"    // optional
  },  
  "background" : {
    "page": "keep.html"
  },
  "content_scripts" : [
    {
      "matches" : [
        "http://www.google.com/*",
        "https://www.google.com/*",
        "http://www.google.co.il/*",
        "https://www.google.co.il/*"
      ],
      "js" : ["jquery-1.7.2.min.js", "google_inject.js"],
      "css": ["keepit.css"],
      "run_at" : "document_end",//document_start
      "all_frames" : false
    },
    {
      "matches" : [
        "http://*/*",
        "https://*/*"
      ],
      "js" : ["jquery-1.7.2.min.js", "jquery-ui-1.9.1.custom.min.js", "jquery.range.js", "contentscript.js", "mustache.js", "hover.js"],
      "css": ["keepit.css", "hover.css", "social_window.css"],
      "run_at" : "document_end",
      "all_frames" : false
    }
  ],
  "permissions": [
    "tabs",
    "cookies",
    "bookmarks",
    "http://*/",
    "https://*/",
    "https://www.netwallet.com/",
    "http://www.netwallet.com/",
    "unlimitedStorage"
  ],
  "web_accessible_resources": [
    "arrow.png",
    "bookmarks.js",
    "contentscript.js",
    "downarrow_hover.png",
    "downarrow.png",
    "google_inject.html",
    "hover.css",
    "hover.js",
    "jquery-1.7.2.min.js",
    "jquery-ui-1.9.1.custom.min.js",
    "keep.html",
    "keepit.css",
    "keepit.png",
    "kept_hover.html",
    "kept.png",
    "kifi.png",
    "kifilogo.png",
    "magicQueries.json",
    "mustache.js",
    "options.html",
    "social_hover.html",
    "social_window.css",
    "social_window.html",
    "social_window.js",
    "social-icons.png",
    "uparrow_hover.png",
    "uparrow.png",
    "URI.js",
    "icons/dropbox.png",
    "icons/facebook.png",
    "icons/gdocs.gif",
    "icons/gdrive.png",
    "icons/gmail.png"
  ] 
}
